{
  "$id": "https://coherosphere.com/schemas/metrics/SRIComputed.v1.json",
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "type": "object",
  "properties": {
    "sri": {
      "type": "number"
    },
    "window": {
      "type": "string",
      "enum": [
        "1h",
        "24h",
        "7d",
        "30d"
      ]
    },
    "basis": {
      "type": "string",
      "enum": [
        "events",
        "projects",
        "hubs"
      ]
    },
    "at": {
      "type": "string",
      "format": "date-time"
    }
  },
  "required": [
    "sri",
    "window",
    "basis",
    "at"
  ],
  "additionalProperties": false
}