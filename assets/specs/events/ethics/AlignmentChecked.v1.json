{
  "$id": "https://coherosphere.com/schemas/ethics/AlignmentChecked.v1.json",
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "type": "object",
  "properties": {
    "subjectId": {
      "$ref": "../common.json#/$defs/ID"
    },
    "rubricId": {
      "type": "string"
    },
    "alignment": {
      "type": "number",
      "minimum": 0,
      "maximum": 1
