{
  "$id": "https://schemas.coherosphere.io/events/security/IncidentOpened.v1.json",
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "Security.IncidentOpened",
  "version": "1.0.0",
  "type": "object",
  "properties": {
    "incidentId": {
      "$ref": "../_meta/common.json#/$defs/ID"
    },
    "severity": {
      "type": "string",
      "enum": [
        "Low",
        "Medium",
        "High",
        "Critical"
      ]
    },
    "openedAt": {
      "type": "string",
      "format": "date-time"
    }
  },
  "required": [
    "incidentId",
    "severity",
    "openedAt"
  ],
  "additionalProperties": false
}