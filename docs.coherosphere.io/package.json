{
  "scripts": {
    "clean": "rimraf build .docusaurus",
    "prebuild": "npm run clean && mkdir -p static/assets && rsync -a ../assets/ static/assets/ && node ./scripts/gen-asset-indexes.mjs || true",
    "build": "node ./node_modules/@docusaurus/core/bin/docusaurus.mjs build",
    "start": "node ./node_modules/@docusaurus/core/bin/docusaurus.mjs start",
    "serve": "node ./node_modules/@docusaurus/core/bin/docusaurus.mjs serve"
  },
  "dependencies": {
    "@docusaurus/core": "3.9.2",
    "@docusaurus/preset-classic": "3.9.2",
    "@docusaurus/theme-mermaid": "3.9.2",
    "@docusaurus/plugin-client-redirects": "3.9.2",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "rimraf": "5.0.10"
  }
}