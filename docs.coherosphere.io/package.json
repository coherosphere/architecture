    "rebuild": "npm run clean && npm install && npm run prebuild && npm run build"
  },
  "dependencies": {
    "@docusaurus/core": "3.9.2",
    "@docusaurus/preset-classic": "3.9.2",
    "@docusaurus/theme-mermaid": "3.9.2",
    "prism-react-renderer": "2.3.1",
    "react": "18.3.1",
    "react-dom": "18.3.1"
  },
  "overrides": {
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "scheduler": "0.23.2"
  },
  "engines": {
    "node": ">=18.0.0"
  },
